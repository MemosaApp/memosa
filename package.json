{
  "name": "meteor-app",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run build:watch\" \"meteor run --settings settings.json\"",
    "build": "npm run build:scss",
    "build:scss": "gulp build:scss --gulpfile .gulpfile.babel.js",
    "build:watch": "nodemon --watch imports -e scss --exec \"npm run build:scss\"",
    "pretest": "npm run lint --silent",
    "test": "yarn run test:mocha && yarn run test:chimp",
    "test:chimp": "chimp --mocha --ddp=http://localhost:3000 --browser=phantomjs --path=.features",
    "test:mocha": "NODE_ENV=test mocha --compilers js:babel-core/register .test-boot.js imports/**/*.test.js",
    "test:watch": "nodemon --watch imports --watch client --watch server --exec \"npm run test:mocha\"",
    "predeploy": "meteor npm prune",
    "lint": "eslint .",
    "lint:watch": "nodemon --watch imports --watch client --watch server --exec \"npm run lint\"",
    "postinstall": "./.scripts/postinstall.sh"
  },
  "dependencies": {
    "aws-lib": "^0.3.0",
    "babel-runtime": "^6.18.0",
    "bcrypt": "^1.0.2",
    "bottlejs": "^1.5.0",
    "chimp": "^0.47.0",
    "classnames": "^2.2.5",
    "debounce": "^1.0.0",
    "draft-js": "^0.9.1",
    "materialize-css": "^0.97.8",
    "md5": "^2.2.1",
    "moment": "^2.17.1",
    "promise": "^7.1.1",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-pure-render-mixin": "^15.3.2",
    "react-dom": "^15.4.0",
    "react-dropzone": "^3.7.3",
    "react-fab": "^1.0.0",
    "react-fastclick": "^2.1.2",
    "react-hocs": "^1.0.0",
    "react-modal": "^1.5.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "reactionic": "^1.1.2",
    "reactionic-icons": "^1.0.0",
    "reactionic-modal": "^1.0.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-memosa": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-root-import": "^4.1.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.0",
    "chimp": "^0.41.2",
    "concurrently": "^3.1.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.1",
    "eslint-config-memosa": "^1.0.0",
    "eslint-import-resolver-meteor": "^0.3.3",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-no-empty-blocks": "0.0.2",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "fibers": "^1.0.15",
    "gulp": "^3.9.1",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.2.1",
    "jsdoc-memosa": "^1.0.0",
    "jsdoc-to-markdown": "^2.0.1",
    "jsdom": "^9.8.3",
    "meteor-node-stubs": "^0.2.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.4.0",
    "reactionic-mocks": "^1.0.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  }
}
