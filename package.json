{
  "name": "meteor-app",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings.json",
    "lint": "eslint .",
    "pretest": "npm run lint --silent",
    "predeploy": "meteor npm prune",
    "tests-watch": "meteor test --driver-package practicalmeteor:mocha-phantomjs --port 9999",
