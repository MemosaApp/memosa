{
  "name": "meteor-app",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings.json",
    "lint": "eslint .",
    "pretest": "npm run lint --silent",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register .scripts/test-boot.js imports/**/*.test.js",
    "predeploy": "meteor npm prune",
    "watch:meteor": "meteor test --driver-package practicalmeteor:mocha-phantomjs --port 9999",
    "test:meteor": "meteor test --once --driver-package dispatch:mocha-phantomjs --port 9999",
    "watch:integration": "chimp --ddp=http://localhost:3000 --watch --mocha --path=tests",
    "test:integration": "chimp --ddp=http://localhost:3000 --mocha --path=tests --browser=phantomjs",
    "postinstall": "./.scripts/postinstall.sh"
  },
  "dependencies": {
    "aws-lib": "^0.3.0",
    "classnames": "^2.2.5",
    "debounce": "^1.0.0",
    "draft-js": "^0.9.1",
    "materialize-css": "^0.97.8",
    "promise": "^7.1.1",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-pure-render-mixin": "^15.3.2",
    "react-dom": "^15.4.0",
    "react-dropzone": "^3.7.3",
    "react-fastclick": "^2.1.2",
    "react-modal": "^1.5.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "reactionic": "^1.1.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-memosa": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-root-import": "^4.1.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.0",
    "chimp": "^0.41.2",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.1",
    "eslint-config-memosa": "^1.0.0",
    "eslint-import-resolver-meteor": "^0.3.3",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-no-empty-blocks": "0.0.2",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "jsdoc-to-markdown": "^2.0.1",
    "jsdom": "^9.8.3",
    "meteor-node-stubs": "^0.2.4",
    "mocha": "^3.2.0",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.4.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  }
}
